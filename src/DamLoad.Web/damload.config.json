  "Localization": {
    "Locales": [ "en", "fr", "de" ],
    "LocaleFallback": true
  },
  "CustomData": {
    "Fields": {
      "Image": [
        {
          "name": "photographer",
          "isRequired": false
        },
        {
          "name": "resolution",
          "isRequired": true
        }
      ],
      "Video": [
        {
          "name": "director",
          "isRequired": false
        },
        {
          "name": "duration",
          "isRequired": true
        }
      ],
      "Raw": [
        {
          "name": "author",
          "isRequired": false
        },
        {
          "name": "format",
          "isRequired": false
        }
      ]
    }
  },
  "Workflow": {
    "defaultStatus": "Published",
    "finalStatus": "Published",
    "statuses": [
      {
        "name": "Published",
        "description": "Final public state (default on upload)",
        "next": [ "Unpublished", "Archived" ],
        "approval": false,
        "roles": [ "Admin", "Editor" ],
        "storageRoot": "damload-public"
      },
      {
        "name": "Unpublished",
        "description": "Private, not visible to the public",
        "next": [ "Published", "Archived" ],
        "approval": false,
        "roles": [ "Admin", "Editor" ],
        "storageRoot": "damload-private"
      },
      {
        "name": "Archived",
        "description": "Soft-deleted, not visible but can be restored",
        "next": [ "Published", "Unpublished" ],
        "approval": false,
        "roles": [ "Admin" ],
        "storageRoot": "damload-archived"
      }
    ]
  },
  "Variants": {
    "Enabled": true,
    "CreateVariantsOnUpload": [
      {
        "Name": "Copy of uploaded",
        "Transformations": []
      },
      {
        "Name": "Web Standard",
        "Transformations": [
          {
            "Provider": "Cloudinary",
            "Operation": "scale",
            "Parameters": {
              "Width": 1280,
              "Height": 720,
              "Format": "jpeg",
              "Quality": 85
            }
          },
          {
            "Provider": "ImageSharp",
            "Operation": "Crop",
            "Parameters": {
              "Width": 180,
              "Height": 320,
              "PositionX": 25,
              "PositionY": 35
            }
          }
        ]
      }
    ]
  },
  "Transformation": {
    "Providers": [
      {
        "Name": "ImageSharp",
        "Operations": [ "Crop", "Resize", "Rotate" ]
      },
      {
        "Name": "Cloudinary",
        "Operations": [ "scale", "watermark", "enhance" ]
      },
      {
        "Name": "FFmpeg",
        "Operations": [ "convert", "trim", "overlay" ]
      }
    ]
  },
  "Tasks": {
    "UnpublishExpiredAssets": {
      "Type": "cron",
      "Expression": "0 0 * * 1",
      "Priority": 10
    },
    "CleanupTemporaryFiles": {
      "Type": "interval",
      "Interval": "00:30:00",
      "Priority": 5
    },
    "ReindexSearch": {
      "Type": "cron",
      "Expression": "0 6 * * *",
      "Priority": 3
    }
  }
}
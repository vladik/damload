{
  "ConnectionStrings": {
    "DatabaseConnection": "",
    "StorageConnection": ""
  },
  "Storage": {
    "defaultStorageRoot": "Unpublished",
    "StorageRoots": [
      {
        "Status": "Published",
        "Name": "public-assets"
      },
      {
        "Status": "Unpublished",
        "Name": "internal-assets"
      },
      {
        "Status": "Archived",
        "Name": "archived-assets"
      }
    ],
    "CdnBaseUrl": "https://cdn.example.com",
    "CdnConfiguration": {
      "AccountId": "your-sub-id",
      "ResourceId": "resource-group",
      "ProfileName": "cdn-profile",
      "EndpointName": "cdn-endpoint"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}